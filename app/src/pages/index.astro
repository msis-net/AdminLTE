                      <Acceptlistv2 client:visible />
                    </div> 
                  </div>
                  <div class="row">
                    <div class="row mt-3">
                      <ChartAcceptlistv2 client:only="vue" />
                    </div>
                  </div>
                  <div class="row">
                    <div class="row mt-3">
                      <!--
                        <ChartComp client:only="vue" />
                       -->
                      
                    </div>
